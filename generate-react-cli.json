{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "sass",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/components/TemplateName.tsx",
        "style": "templates/components/TemplateName.module.sass"
      },
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/pages/TemplateName.tsx",
        "style": "templates/pages/TemplateName.module.sass"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    },
    "layout": {
      "customTemplates": {
        "component": "templates/layout/TemplateName.tsx",
        "style": "templates/layout/TemplateName.module.sass"
      },
      "path": "src/layout",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    }
  },
  "usesStyledComponents": false
}